		<taxon id="castellii__YLR406C"/>
		<taxon id="castellii__YDL075W"/>
		<taxon id="kluyveri__YLR406C"/>
		<taxon id="duplicationNode__YLR406C"/>
	</taxa>

	<taxa id="postDuplicationSpecies">
		<taxon id="bayanus"/>
		<taxon id="castellii"/>
		<taxon id="cerevisiae"/>
		<taxon id="kudriavzevii"/>
		<taxon id="mikatae"/>
		<taxon id="paradoxus"/>
	</taxa>
	<taxa id="outGroupSpecies">
		<taxon id="kluyveri"/>
	</taxa>

	<taxa id="allPostDuplicationTaxa">
		<taxa idref="postDuplicationSpecies"/>
		<taxon id="duplicationNode"/>
	</taxa>

	<taxa id="allSpecies">
		<taxa idref="postDuplicationSpecies"/>
		<taxa idref="outGroupSpecies"/>
	</taxa>


	<taxa id="allTaxa">
		<taxa idref="allPostDuplicationTaxa"/>
		<taxa idref="outGroupSpecies"/>
	</taxa>




	<!-- The sequence alignment (each sequence refers to a taxon above).         -->
	<!-- ntax=13 nchar=336                                                       -->
	<alignment id="alignment" dataType="nucleotide">
		<sequence>
			<taxon idref="duplicationNode__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="bayanus__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="bayanus__YDL075W"/>
			
		</sequence>
		<sequence>
			<taxon idref="cerevisiae__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="cerevisiae__YDL075W"/>
			
		</sequence>
		<sequence>
			<taxon idref="kudriavzevii__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="kudriavzevii__YDL075W"/>
			
		</sequence>
		<sequence>
			<taxon idref="mikatae__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="mikatae__YDL075W"/>
			
		</sequence>
		<sequence>
			<taxon idref="paradoxus__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="paradoxus__YDL075W"/>
			
		</sequence>
		<sequence>
			<taxon idref="castellii__YLR406C"/>
			
		</sequence>
		<sequence>
			<taxon idref="castellii__YDL075W"/>
			
		</sequence>
		<sequence>
			<taxon idref="kluyveri__YLR406C"/>
			
		</sequence>
	</alignment>

	<convert id="codonAlignment" dataType="codon" geneticCode="universal">
        <alignment idref="alignment"/>
    </convert>


	<!-- The unique patterns from 1 to end                                       -->
	<!-- npatterns=816                                                           -->
	<patterns id="patterns" from="1" strip="false">
		<alignment idref="codonAlignment"/>
	</patterns>

	<pairedParalogSitePatterns id="pairedPatterns" idSeparator="__"	paralogs="YLR406C YDL075W" singleCopySpecies="kluyveri duplicationNode">
		<patterns idref="patterns"/>
		<species>
			<taxa idref="allTaxa"/>
		</species>
	</pairedParalogSitePatterns>


    <newick id="startingTree">
		((((((cerevisiae:0.01,paradoxus:0.01)N5:0.01,mikatae:0.02)N4:0.01,kudriavzevii:0.03)N3:0.01,bayanus:0.04)N2:0.01,castellii:0.05)N1:0.02,kluyveri:0.07)N0;
	</newick>

<!--
	<empiricalTreeDistributionModel id="startingTree" fileName="assignmentTree.trees">
		<taxa idref="species"/>
	</empiricalTreeDistributionModel>
 -->

	<!-- Generate a tree model                                                   -->
	<treeModel id="treeModel">
		<newick idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>



	<!-- Statistic for sum of the branch lengths of the tree (tree length)       -->
	<treeLengthStatistic id="treeLength">
		<treeModel idref="treeModel"/>
	</treeLengthStatistic>

	<duplicationTreeModel id="duplicationTreeModel">
		<treeModel idref="treeModel"/>
		<duplication>
			<taxon idref="duplicationNode"/>
			<parameter id="pseudoBranchLength1" value="0.000" lower="0.0"/>
 			<mrca>
	 			<taxa idref="postDuplicationSpecies"/>
 			</mrca>
		</duplication>
		<duplicationTimeRatio>
			<parameter id="time.proportion.to.duplication" value="0.5" lower="0.0" upper="1.0"/>
		</duplicationTimeRatio>
	</duplicationTreeModel>

	<!-- The strict clock (Uniform rates across branches)                        -->
	<strictClockBranchRates id="branchRates">
		<rate>
			<parameter id="clock.rate" value="1.0" lower="0.0"/>
		</rate>
	</strictClockBranchRates>

	<rateStatistic id="meanRate" name="meanRate" mode="mean" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<strictClockBranchRates idref="branchRates"/>
	</rateStatistic>


    <!-- Frequence Model -->
	<frequencyModel id="freqModel" dataType="codon-universal" codonFromNucleotide="true">
		<alignment idref="alignment"/>
		<!-- <alignment idref="codonAlignment"/> -->
		<frequencies>
			<parameter id="nucleotidefrequencies" dimension="4"/>
		</frequencies>
     </frequencyModel>

   <!-- Codon Model -->
    <museGautCodonModel id="mg" geneticCode="universal">
        <alpha>
            <parameter id="alpha" value="0.1" lower="0"/>
        </alpha>
        <beta>
            <parameter id="beta" value="0.5" lower="0"/>
        </beta>
        <kappa>
            <parameter id="kappa" value="1.5" lower="0"/>
        </kappa>
        <frequencyModel idref="freqModel"/>
    </museGautCodonModel>

	<pairedParalogGeneConversionSubstitutionModel id="igcModel"  numParalog="2">
		<museGautCodonModel idref="mg"/>
		<parameter id="gene.conversion.rate" value="1.0" lower="0.0"/>
	</pairedParalogGeneConversionSubstitutionModel>

	<pairedParalogGeneConversionSubstitutionModel id="baseModel"  numParalog="1">
		<museGautCodonModel idref="mg"/>
		<parameter idref="gene.conversion.rate"/>
	</pairedParalogGeneConversionSubstitutionModel>


	<!-- site model                                                              -->
	<siteModel id="igcSiteModel">
		<substitutionModel>
			<pairedParalogGeneConversionSubstitutionModel idref="igcModel"/>
		</substitutionModel>
		<gammaShape gammaCategories="4">
			<parameter id="siteModel.alpha" value="0.1" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<siteModel id="baseSiteModel">
		<substitutionModel>
			<pairedParalogGeneConversionSubstitutionModel idref="baseModel"/>
		</substitutionModel>
		<gammaShape gammaCategories="4">
			<parameter idref="siteModel.alpha"/>
		</gammaShape>
	</siteModel>

	<pairedParalogBranchmodel id="branchModel">
		<base>
			<pairedParalogGeneConversionSubstitutionModel idref="baseModel"/>
		</base>
		<geneConversion>
			<pairedParalogGeneConversionSubstitutionModel idref="igcModel"/>
		</geneConversion>
		<duplicationTreeModel idref="duplicationTreeModel"/>
		<taxa idref="allPostDuplicationTaxa"/>
	</pairedParalogBranchmodel>


	<!-- Likelihood for tree given sequence data                                 -->
	<treeDataLikelihood id="treeLikelihood" useAmbiguities="false"  scalingScheme="always" delayScaling="false">
		<pairedParalogSitePatterns idref="pairedPatterns"/>
		<siteModel idref="baseSiteModel"/>
		<duplicationTreeModel idref="duplicationTreeModel"/>
		<strictClockBranchRates idref="branchRates"/>
		<pairedParalogBranchmodel idref="branchModel"/>
	</treeDataLikelihood>

<!-- 
	<report>
		<treeDataLikelihood idref="treeLikelihood"/>
	</report>
 -->

	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="default">
		<scaleOperator scaleFactor="0.75" weight="5">
			<parameter idref="kappa"/>
		</scaleOperator>

		<deltaExchange delta="0.01" weight="5">
			<parameter idref="nucleotidefrequencies"/>
		</deltaExchange>


		<scaleOperator scaleFactor="0.75" weight="2">
			<parameter idref="gene.conversion.rate"/>
		</scaleOperator>

		<scaleOperator scaleFactor="0.75" weight="1.0">
			<parameter idref="siteModel.alpha"/>
		</scaleOperator>
		<uniformOperator weight="1">
			<parameter idref="time.proportion.to.duplication"/>
		</uniformOperator>


<!--
		<subtreeSlide size="1.0" gaussian="true" weight="30">
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="30">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="3">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="3">
			<treeModel idref="treeModel"/>
		</wilsonBalding>
 -->
		<scaleOperator scaleFactor="0.75" scaleAll="true" ignoreBounds="true" weight="3">
			<parameter idref="treeModel.allInternalNodeHeights"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="3">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>


	</operators>


	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="10000000" autoOptimize="false" operatorAnalysis="geneConversionTest.ops" fullEvaluation="100">
		<joint id="joint">
			<prior id="prior">
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="kappa"/>
				</logNormalPrior>
				<dirichletPrior alpha="1.0" sumsTo="1.0">
					<parameter idref="nucleotidefrequencies"/>
				</dirichletPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="gene.conversion.rate"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="siteModel.alpha"/>
				</exponentialPrior>
<!-- 				<strictClockBranchRates idref="branchRates"/> -->
			</prior>

			<likelihood id="likelihood">
				<treeDataLikelihood idref="treeLikelihood"/>
			</likelihood>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="1000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
<!--
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
 -->
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="frequency" dp="8" width="12">
				<parameter idref="nucleotidefrequencies"/>
			</column>
			<column label="alpha" dp="4" width="12">
				<parameter idref="siteModel.alpha"/>
			</column>
			<column label="kappa" dp="4" width="12">
				<parameter idref="kappa"/>
			</column>
			<column label="tau" dp="4" width="12">
				<parameter idref="gene.conversion.rate"/>
			</column>
			<column label="dup.proportion" dp="4" width="12">
				<parameter idref="time.proportion.to.duplication"/>
			</column>
			<column label="nodeHeight" sf="6" width="12">
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1" fileName="geneConversionTest.log" overwrite="false">
			<joint idref="joint"/>
<!-- 			<prior idref="prior"/> -->
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<treeLengthStatistic idref="treeLength"/>
			<parameter idref="nucleotidefrequencies"/>
			<parameter idref="kappa"/>
			<parameter idref="siteModel.alpha"/>
			<parameter idref="gene.conversion.rate"/>
			<parameter idref="time.proportion.to.duplication"/>
			<parameter idref="treeModel.allInternalNodeHeights"/>
			<parameter idref="clock.rate"/>
			<rateStatistic idref="meanRate"/>
			<strictClockBranchRates idref="branchRates"/>
		</log>

		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="1000" nexusFormat="true" fileName="geneConversionTest.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<trait name="rate" tag="rate">
				<strictClockBranchRates idref="branchRates"/>
			</trait>
			<joint idref="joint"/>
		</logTree>
	</mcmc>

	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>




</beast>

		<taxon id="cerevisiae__YLR406C"/>
		<taxon id="cerevisiae__YDL075W"/>
		<taxon id="kudriavzevii__YLR406C"/>
		<taxon id="kudriavzevii__YDL075W"/>
		<taxon id="mikatae__YLR406C"/>
		<taxon id="mikatae__YDL075W"/>
		<taxon id="paradoxus__YLR406C"/>
